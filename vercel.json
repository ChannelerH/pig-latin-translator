{
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "Content-Security-Policy",
            "value": "default-src 'self' https: 'unsafe-inline' 'unsafe-eval'; script-src 'self' https: 'unsafe-inline' 'unsafe-eval' https://*.effectiveratecpm.com; frame-ancestors 'self' https://piglatintranslator.pro"
          },
          {
            "key": "X-Frame-Options",
            "value": "ALLOW-FROM https://piglatintranslator.pro"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }