{
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "Content-Security-Policy",
            "value": "frame-ancestors 'self' https://piglatintranslator.pro; upgrade-insecure-requests"
          },
          {
            "key": "X-Frame-Options",
            "value": "ALLOW-FROM https://piglatintranslator.pro"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }